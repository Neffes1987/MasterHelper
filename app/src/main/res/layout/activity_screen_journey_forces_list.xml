<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="com.example.com.masterhelper.ui.forces.ScreenJourneyForcesList">

  <fragment
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:name="com.example.com.masterhelper.core.factorys.list.ListFactory"
    android:id="@+id/FORCES_LIST_ID"
    tools:layout="@layout/fragment_recycler_view"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"/>

  <com.google.android.material.floatingactionbutton.FloatingActionButton
    android:src="@mipmap/baseline_add_black_18dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:id="@+id/FORCES_CREATE_NEW_ID"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    style="@style/float_button_styles"/>
</androidx.constraintlayout.widget.ConstraintLayout>
